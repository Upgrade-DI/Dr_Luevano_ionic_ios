<?xml version='1.0' encoding='utf-8'?>
<widget id="com.armoniaesttica.app" version="1.0.71" xmlns="https://www.w3.org/ns/widgets" xmlns:gap="https://phonegap.com/ns/1.0">
    <name>Dr. Luevano</name>
    <description>
    </description>
    <author email="info@upgrade.com.mx" href="upgrade.com.mx">
	Upgrade Dise√±o Interactivo
	</author>
    <platform name="ios">
        <resource-file src="app/GoogleService-Info.plist" target="app/GoogleService-Info.plist" />
    </platform>
    <preference name="cordova-cli" value="cli-9.0.0" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="Disallowsscroll" value="true" />
    <preference name="disallowOverscroll" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreenDelay" value="1200" />
    <preference name="FadeSplashScreenDuration" value="200" />
    <preference name="target-device" value="handset" />
    <preference name="deployment-target" value="13.1" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="detect-data-types" value="true" />
    <preference name="prerendered-icon" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="KeepRunning" value="true" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <platform name="ios">
        <icon height="29" src="resources/ios/icon/29x29.png" width="29" />
        <icon height="40" src="resources/ios/icon/40x40.png" width="40" />
        <icon height="50" src="resources/ios/icon/50x50.png" width="50" />
        <icon height="57" src="resources/ios/icon/57x57.png" width="57" />
        <icon height="58" src="resources/ios/icon/58x58.png" width="58" />
        <icon height="60" src="resources/ios/icon/60x60.png" width="60" />
        <icon height="72" src="resources/ios/icon/72x72.png" width="72" />
        <icon height="76" src="resources/ios/icon/76x76.png" width="76" />
        <icon height="80" src="resources/ios/icon/80x80.png" width="80" />
        <icon height="87" src="resources/ios/icon/87x87.png" width="87" />
        <icon height="114" src="resources/ios/icon/114x114.png" width="114" />
        <icon height="100" src="resources/ios/icon/100x100.png" width="100" />
        <icon height="180" src="resources/ios/icon/180x180.png" width="180" />
        <icon height="144" src="resources/ios/icon/144x144.png" width="144" />
        <icon height="120" src="resources/ios/icon/120x120.png" width="120" />
        <icon height="152" src="resources/ios/icon/152x152.png" width="152" />
        <icon height="1024" src="resources/ios/icon/1024x1024.png" width="1024" />
    </platform>
    <platform name="ios">
        <splash src="resources/ios/splash/Default@2x~universal~anyany.png" />
    </platform>
    <preference name="permissions" value="none" />
    <plugin name="cordova-plugin-camera" source="npm" spec="~5.0.1" />
    <plugin name="cordova-plugin-device" source="npm" spec="~2.0.3" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~2.0.2" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~4.1.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="~6.0.0" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.3" />
    <plugin name="cordova-plugin-qrscanner-dual" spec="~3.0.8" />
    <plugin name="cordova-plugin-test-framework" source="npm" spec="~1.1.6" />
    <plugin name="cordova-plugin-vibration" source="npm" spec="~2.1.1" />
    <engine name="cordova-browser" spec="~6.0.0" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.3.4" />
    <plugin name="pushwoosh-cordova-plugin" spec="~8.0.0">
        <variable name="LOG_LEVEL" value="DEBUG" />
        <variable name="IOS_FOREGROUND_ALERT_TYPE" value="BANNER" />
        <variable name="ANDROID_FOREGROUND_PUSH" value="false" />
    </plugin>
    <access origin="*" subdomains="true" />
    <access origin="*.pushwoosh.com" />
    <access launch-external="yes" origin="whatsapp:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="whatsapp:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
